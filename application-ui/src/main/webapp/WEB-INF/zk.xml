		<label-location>/WEB-INF/i18n/error_msg.properties</label-location>
		<label-location>/WEB-INF/i18n/confirm_msg.properties</label-location>
		<label-location>/WEB-INF/i18n/zk_custom.properties</label-location>
		<label-location>/WEB-INF/application.properties</label-location>


		<ui-factory-class>org.zkoss.zk.ui.http.SerializableUiFactory
		</ui-factory-class>
	</system-config>
	
	 <language-config>
	    <addon-uri>/WEB-INF/lang-addon.xml</addon-uri>        
	</language-config>

	<library-property>
		<name>org.zkoss.web.preferred.locale</name>
		<value>in</value>
	</library-property>

	<library-property>
		<name>org.zkoss.bind.appValidators</name>
		<value>
	email=common.validator.EmailValidator,
	notEmpty=common.validator.NotEmptyValidator,
<!-- 	dateRange=common.validator.DateRangeValidator, -->
	numericRange=common.validator.NumericRangeValidator,
	costumDateRange=common.validator.CostumDateRangeValidator,
<!-- 	percentage=common.validator.PercentageValidator, -->
	numeric=common.validator.NumericValidator,
	numberMaxLength=common.validator.NumberMaxLengthValidator,
	alphabet=common.validator.AlphabetValidator,
<!-- 	integerMaxLength=common.validator.IntegerMaxLengthValidator, -->
	policeNumber=common.validator.PoliceNumberValidator
		</value>
	</library-property>

	<library-property>
		<name>org.zkoss.web.preferred.dateFormatInfo</name>
		<value>common.formatter.DefaultDateFormat</value>
	</library-property>

	<library-property>
		<name>org.zkoss.bind.appConverters</name>
		<value>
			dateConverter=common.formatter.DateConverterUtil,
			decimalConverter=common.formatter.DecimalConverterUtil,
			dateTimeConverter=common.formatter.DateTimeConverterUtil,
			bigDecimalConverter=common.formatter.BigDecimalConverterUtil

		</value>
	</library-property>

	<listener>
		<listener-class>org.zkoss.zkmax.au.InaccessibleWidgetBlockService$DesktopInit
		</listener-class>
	</listener>

	<library-property>
		<name>org.zkoss.zkmax.au.IWBS.events</name>
		<value>onClick,onChange,onSelect</value>
	</library-property>

