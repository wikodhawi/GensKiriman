<?xml version="1.0" encoding="UTF-8"?>
<zk>
	<system-config>

		<label-location>/WEB-INF/common_app.properties</label-location>
		<label-location>/WEB-INF/i18n/app_lbl.properties</label-location>
		<label-location>/WEB-INF/i18n/general_lbl.properties</label-location>
