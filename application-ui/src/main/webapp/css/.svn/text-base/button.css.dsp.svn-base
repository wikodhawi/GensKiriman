<%@ taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c" %><%@ taglib uri="http://www.zkoss.org/dsp/zk/core" prefix="z" %><%@ taglib uri="http://www.zkoss.org/dsp/web/theme" prefix="t" %>.z-button{font-family:'Open Sans';font-size:12px;font-weight:normal;font-style:normal;color:#ffffff;min-height:24px;border:1px solid #0095d7;${t:borderRadius('4px')};padding:3px 12px;line-height:14px;background:#0095d7;text-shadow:none;cursor:pointer;white-space:nowrap}.z-button:hover{border-color:#8fb9d0;background:#00c0d7}.z-button:focus{color:#ffffff;border-color:#0cbcff;background:#0cbcff;${t:boxShadow('inset 0 0 2px #0cbcff')}}.z-button:active{border-color:#499eb3;background:#00b1ff}.z-button[disabled]{border-color:#a9a9a9;background:#80c1e9;opacity:.6;filter:alpha(opacity=60);${t:boxShadow('none')};cursor:default}.z-button img{vertical-align:middle}.z-button.btn-lov{height:24px;width:24px;border-radius:24px;padding:0!important}.z-button.btn-index{background:none!important;border:none!important;color:#0095d7!important;width:35px!important;padding:3px!important}.z-button.btn-index:hover{color:#36f!important}.z-button.btn-index:active{color:#69f!important}.z-button.btn-index:focus{color:#66f!important}.z-button.btn-index[disabled]{color:#337373!important}.z-button.btn-index>[class*="z-icon-"]{display:inline;margin-right:4px;margin-left:4px;font-size:large}.z-button.btn-gold{background:#c28b45;border-color:#cfaf4d}.z-button.btn-gold:hover{background-color:#cfaf4d;border-color:#fff}.z-button.btn-gold:active{background:#cf9a4d!important;border-color:#fff!important}.z-button.btn-gold:focus{background:#e8a13b;border-color:#fff}.z-button.btn-gold[disabled]{background:#d6ac75;border:#fff}.z-button.btn-white{background:#fff;border-color:#ddd;color:#c28b45}.z-button.btn-white:hover{background-color:#ddd;border-color:#fff}.z-button.btn-white:active{background:#fde!important;border-color:#fff!important}.z-button.btn-white:focus{background:#eef;border-color:#fff}.z-button.btn-white[disabled]{background:#ccc;border:#fff}.z-button.btn-lov-white{background:#fff;border-color:#ddd;height:24px;width:24px;border-radius:24px;color:#c28b45;padding:0!important}.z-button.btn-lov-white:hover{background-color:#ddd;border-color:#fff}.z-button.btn-lov-white:active{background:#fde!important;border-color:#fff!important}.z-button.btn-lov-white:focus{background:#eef;border-color:#fff}.z-button.btn-lov-white[disabled]{background:#ccc;border:#fff}.z-button.btn-trans{background-color:#fff;border-color:#fff;color:#c28b45}.z-button.btn-trans:hover{color:#db9a47}.z-button.btn-trans:focus{color:#0066ea}.z-button.btn-trans:active{color:#62a8d1}.z-button.btn-trans[disabled]{color:#d6ac75}.ie8 .z-button{min-height:16px}